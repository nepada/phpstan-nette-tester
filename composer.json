{
    "name": "nepada/phpstan-nette-tester",
    "type": "phpstan-extension",
    "license": "BSD-3-Clause",
    "description": "PHPStan nette/tester extension",
    "keywords": [
        "phpstan",
        "nette",
        "tester"
    ],
    "support": {
        "email": "petr@pada.cz",
        "issues": "https://github.com/nepada/phpstan-nette-tester/issues"
    },
    "authors": [
        {
            "name": "Petr MorÃ¡vek",
            "email": "petr@pada.cz"
        }
    ],
    "require": {
        "php": ">=7.2 <8.2",
        "nikic/php-parser": "^4.13.1",
        "phpstan/phpstan": "^1.0"
    },
    "require-dev": {
        "nepada/coding-standard": "6.4.0",
        "php-parallel-lint/php-parallel-lint": "1.3.1",
        "nette/tester": "2.4.1",
        "phpstan/phpstan-phpunit": "1.0.0",
        "phpstan/phpstan-strict-rules": "1.1.0",
        "phpunit/phpunit": "^8.5.21 || ^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "Nepada\\PHPStan\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NepadaTests\\PHPStan\\": "tests/"
        }
    },
    "config": {
        "platform-check": false
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.4-dev"
        },
        "phpstan": {
            "includes": [
                "extension.neon"
            ]
        }
    }
}
