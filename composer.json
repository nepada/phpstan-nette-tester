{
    "name": "nepada/phpstan-nette-tester",
    "type": "phpstan-extension",
    "license": "BSD-3-Clause",
    "description": "PHPStan nette/tester extension",
    "keywords": [
        "phpstan",
        "nette",
        "tester"
    ],
    "support": {
        "email": "petr@pada.cz",
        "issues": "https://github.com/nepada/phpstan-nette-tester/issues"
    },
    "authors": [
        {
            "name": "Petr MorÃ¡vek",
            "email": "petr@pada.cz"
        }
    ],
    "require": {
        "php": ">=7.4 <8.4",
        "nikic/php-parser": "^4.13.2",
        "phpstan/phpstan": "^1.8.11"
    },
    "require-dev": {
        "nepada/coding-standard": "7.13.0",
        "php-parallel-lint/php-parallel-lint": "1.3.2",
        "nette/tester": "^2.4",
        "phpstan/phpstan-phpunit": "1.3.15",
        "phpstan/phpstan-strict-rules": "1.5.2",
        "spaze/phpstan-disallowed-calls": "2.16.1",
        "shipmonk/phpstan-rules": "2.9.2",
        "phpunit/phpunit": "^8.5.21 || ^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "Nepada\\PHPStan\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NepadaTests\\PHPStan\\": "tests/"
        }
    },
    "config": {
        "platform-check": false,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        },
        "phpstan": {
            "includes": [
                "extension.neon"
            ]
        }
    }
}
