{
    "name": "nepada/phpstan-nette-tester",
    "type": "phpstan-extension",
    "license": "BSD-3-Clause",
    "description": "PHPStan nette/tester extension",
    "keywords": [
        "phpstan",
        "nette",
        "tester"
    ],
    "support": {
        "email": "petr@pada.cz",
        "issues": "https://github.com/nepada/phpstan-nette-tester/issues"
    },
    "authors": [
        {
            "name": "Petr MorÃ¡vek",
            "email": "petr@pada.cz"
        }
    ],
    "require": {
        "php": ">=7.4 <8.6",
        "nikic/php-parser": "^4.13.2 || ^v5.0.2",
        "phpstan/phpstan": "^2.0"
    },
    "require-dev": {
        "nepada/coding-standard": "8.0.1",
        "php-parallel-lint/php-parallel-lint": "1.4.0",
        "nette/tester": "^2.4",
        "phpstan/phpstan-phpunit": "2.0.7",
        "phpstan/phpstan-strict-rules": "2.0.6",
        "spaze/phpstan-disallowed-calls": "4.6.0",
        "shipmonk/phpstan-rules": "4.2.0",
        "phpunit/phpunit": "^9.6.19"
    },
    "autoload": {
        "psr-4": {
            "Nepada\\PHPStan\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NepadaTests\\PHPStan\\": "tests/"
        }
    },
    "config": {
        "platform-check": false,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        },
        "phpstan": {
            "includes": [
                "extension.neon"
            ]
        }
    }
}
