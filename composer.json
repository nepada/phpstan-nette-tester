{
    "name": "nepada/phpstan-nette-tester",
    "type": "phpstan-extension",
    "license": "BSD-3-Clause",
    "description": "PHPStan nette/tester extension",
    "keywords": [
        "phpstan",
        "nette",
        "tester"
    ],
    "support": {
        "email": "petr@pada.cz",
        "issues": "https://github.com/nepada/phpstan-nette-tester/issues"
    },
    "authors": [
        {
            "name": "Petr MorÃ¡vek",
            "email": "petr@pada.cz"
        }
    ],
    "require": {
        "php": ">=7.2",
        "nikic/php-parser": "^4.0",
        "phpstan/phpstan": "^0.12.54"
    },
    "require-dev": {
        "nepada/coding-standard": "^6.4.0",
        "php-parallel-lint/php-parallel-lint": "^1.2",
        "nette/tester": "^2.3.2",
        "phpunit/phpunit": "^8.5.9 || ^9.4.3",
        "phpstan/phpstan-phpunit": "^0.12.16",
        "phpstan/phpstan-strict-rules": "^0.12.5"
    },
    "autoload": {
        "psr-4": {
            "Nepada\\PHPStan\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NepadaTests\\": "tests/"
        }
    },
    "config": {
        "platform-check": false
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3-dev"
        },
        "phpstan": {
            "includes": [
                "extension.neon"
            ]
        }
    }
}
